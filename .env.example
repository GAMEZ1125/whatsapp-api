# Configuración del Servidor
PORT=3000
NODE_ENV=development

# API Keys para autenticación (generar claves seguras)
API_KEY=tu-api-key-segura-aqui
API_SECRET=tu-api-secret-aqui

# Configuración de WhatsApp
SESSION_NAME=whatsapp-session
HEADLESS=true

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logs
LOG_LEVEL=info
